body {
  background: #e94626;
  background: linear-gradient(90deg, rgba(233, 70, 38, 0.453427065) 0%, #e2dbdb 50%, rgba(233, 70, 38, 0.45) 100%);
  height: auto;
  line-height: 1.5;
}

.head {
  font-family: "Londrina Shadow", cursive;
  font-size: 48px;
  margin: 0;
  margin-left: 20px;
}

.logo {
  width: 200px;
  display: block;
  background-color: lightgrey;
  border-radius: 10rem;
  margin-left: 50px;
}

.text-title {
  font-size: 32px;
  font-family: "Yeseva One", cursive;
  width: 500px;
}