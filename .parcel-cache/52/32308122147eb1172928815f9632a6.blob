body {
  background: black;
  min-height: 100vh;
  background-repeat: no-repeat;
}

header {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  margin-top: 15px;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

header ul {
  display: flex;
  justify-content: center;
  align-items: center;
}

header ul li {
  list-style: none;
  margin-left: 20px;
}

header ul li a {
  text-decoration: none;
  padding: 6px 15px;
  color: white;
  font-family: "Poiret One", cursive;
}

.head {
  font-family: "Yeseva One";
  color: lightyellow;
  font-size: 90px;
  margin-top: 50px;
}

.logo {
  width: 400px;
  display: block;
  border-radius: 50rem;
  margin-left: auto;
  margin-right: auto;
}

.text-title {
  font-family: "Playfair Display SC", serif;
  color: white;
  font-size: 100px;
  padding: 1em;
}

.text-Decs {
  font-family: "DM Sans", sans-serif;
  color: white;
  font-size: 45px;
  padding: 1em;
}

.phone {
  font-size: 12px;
  color: oldlace;
  font-family: "Poiret One", cursive;
}
